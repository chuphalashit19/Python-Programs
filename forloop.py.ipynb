    "# forloop.py\n",
    "\n",
    "start = int(input(\"Enter the start: \"))\n",
    "end = int(input(\"Enter the end: \"))\n",
    "\n",
    "for n in range(start, end + 1):\n",
    "    if n > 1:\n",
    "        for i in range(2, n):\n",
    "            if (n % i) == 0:\n",
    "                break\n",
    "        else:\n",
    "            print(\"Prime Number:\", n)"
   ]
